                <library dirs="200">
                    <file>${file.reference.javax.mail-1.5.6.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.activation.jar-1}</file>
